{
  "name": "json5-server",
  "version": "0.1.0",
  "exports": "./main.ts",
  "tasks": {
    "start": "deno run --allow-net --allow-read --allow-import main.ts",
    "install": "deno install --allow-net --allow-read --allow-import -n json5-server main.ts",
    "build": "deno compile --allow-net --allow-read --allow-import -o json5-server main.ts"
  },
  "imports": {
    "@std/assert": "jsr:@std/assert@1",
    "json5": "https://deno.land/x/json5@v1.0.0/mod.ts",
    "std/cli": "https://deno.land/std@0.224.0/cli/mod.ts"
  }
}
